{
  "name": "cf-nodejs-logging-support",
  "version": "8.0.0",
  "description": "Logging tool for Cloud Foundry",
  "keywords": [
    "logging",
    "cloud-foundry"
  ],
  "main": "build/main/index.js",
  "types": "build/main/index.d.ts",
  "typings": "build/main/index.d.ts",
  "module": "build/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/cf-nodejs-logging-support"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Christian Dinse, Nicklas Dohrn, Federico Romagnoli"
  },
  "homepage": "https://sap.github.io/cf-nodejs-logging-support/",
  "scripts": {
    "build": "npm run build-json-schema && tsc -p tsconfig.json",
    "build-json-schema": "typescript-json-schema 'src/lib/config/interfaces.ts' ConfigObject --noExtraProps --required --out 'src/lib/config/default/config-schema.json'",
    "prepare": "npm run build",
    "test": "npm run build && node build/main/index.js && mocha 'src/test/**/*.test.js'",
    "test-lint": "eslint src --ext .ts",
    "test-performance": "mocha 'src/performance-test/*.test.js'",
    "test-coverage": "nyc --reporter=lcov --reporter=text-summary npm run test"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/json-stringify-safe": "^5.0.3",
    "@types/node": "^20.14.8",
    "@types/triple-beam": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "chai": "^6.2.2",
    "connect": "^3.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "express": "^5.2.1",
    "fastify": "^5.7.4",
    "import-fresh": "^3.3.1",
    "mocha": "^11.3.0",
    "node-mocks-http": "^1.17.2",
    "nyc": "^17.1.0",
    "rewire": "^9.0.1",
    "supertest": "^7.2.2",
    "typescript": "^5.9.3",
    "typescript-json-schema": "^0.67.1"
  },
  "files": [
    "/.reuse/",
    "/build/",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "ajv": "^8.17.1",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^9.0.3",
    "triple-beam": "^1.4.1",
    "uuid": "^13.0.0",
    "winston-transport": "^4.9.0"
  }
}
