{
  "name": "cf-nodejs-logging-support",
  "version": "7.0.0",
  "description": "Logging tool for Cloud Foundry",
  "keywords": [
    "logging",
    "cf",
    "perfx"
  ],
  "main": "build/main/index.js",
  "types": "build/main/index.d.ts",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/cf-nodejs-logging-support"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Federico Romagnoli, Christian Dinse"
  },
  "homepage": "https://sap.github.io/cf-nodejs-logging-support/",
  "scripts": {
    "build": "npm run build-json-schema && tsc -p tsconfig.json",
    "start": "npm run build-json-schema && tsc -p tsconfig.json && node build/main/index.js",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "mocha 'src/test/**/*.test.js'",
    "test:lint": "eslint src --ext .ts",
    "build-json-schema": "typescript-json-schema 'src/lib/config/interfaces.ts' ConfigObject --noExtraProps --out 'src/lib/config/config-schema.json'"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.45",
    "@types/restify": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^3.10.1",
    "chai": "^4.3.6",
    "eslint": "^8.20.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "import-fresh": "^3.3.0",
    "mocha": "^6.2.3",
    "supertest": "^6.2.2",
    "ts-node": "^9.0.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.0.2",
    "typescript-json-schema": "^0.53.0"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "ajv": "^8.11.0",
    "connect": "^3.7.0",
    "express": "^4.17.3",
    "http": "^0.0.1-security",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "restify": "^8.6.1"
  }
}
